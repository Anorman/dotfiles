[init]
  templatedir = ~/.git-template
[merge]
  tool = vimdiff
[user]
    name         = Jared Norman
    email        = jared@freerunningtechnologies.com
[github]
    user = jarednorman
[alias]
    sup = "!git status -s && git l -5"
    d   = diff --color
    hp  = "!. ~/.githelpers && show_git_head"
    ff  = merge --ff-only
    l   = "!. ~/.githelpers && pretty_git_log"
    c   = commit -v
    ca  = commit -v --amend
[color]
    branch       = auto
    diff         = auto
    status       = auto
[color "branch"]
    current      = yellow reverse
    local        = yellow
    remote       = green
[color "diff"]
    meta         = blue
    frag         = yellow
    old          = red
    new          = green
[color "status"]
    added        = blue bold
    changed      = yellow
    untracked    = cyan
