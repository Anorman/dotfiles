[user]
	name = Jared Norman
    email  = hi@jarednorman.ca
[alias]
    head   = !git l -1
    d      = diff --color
    h      = !git head
    hp     = "!source ~/.githelpers && show_git_head"
    ff     = merge --ff-only
    pullff = pull --ff-only
    l      = "!source ~/.githelpers && pretty_git_log"
    stat   = status
    com    = commit
    cam    = commit -am
    ca     = commit -a
    pom    = push origin master
[color]
    branch = auto
    diff   = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local   = yellow 
    remote  = green
[color "diff"]
    meta   = blue
    frag   = yellow
    old    = red
    new    = green
[color "status"]
    added     = blue bold
    changed   = yellow
    untracked = cyan
